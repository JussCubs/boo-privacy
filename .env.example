# Boo Privacy Environment Variables
# Copy this file to .env.local and fill in your values

# Privy App ID (Required)
# Get your app ID from https://console.privy.io
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# Solana RPC URLs
# Use a premium RPC for best performance (Helius, QuickNode, etc.)
NEXT_PUBLIC_SOLANA_SECURE_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
NEXT_PUBLIC_SOLANA_RPC_URL_PROTECTED=https://api.mainnet-beta.solana.com

# Network (mainnet-beta or devnet)
NEXT_PUBLIC_NETWORK=mainnet-beta
